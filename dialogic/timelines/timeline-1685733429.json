		{
			"choice": "Muy bien, creo que está todo chupado.",
			"condition": "",
			"definition": "",
			"event_id": "dialogic_011",
			"value": ""
		},
		{
			"character": "character-1685734503.json",
			"event_id": "dialogic_001",
			"portrait": "",
			"text": "Oh. Eso es tener mucha confianza..."
