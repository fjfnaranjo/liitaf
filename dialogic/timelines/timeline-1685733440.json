		},
		{
			"character": "character-1685734503.json",
			"event_id": "dialogic_001",
			"portrait": "",
			"text": "¿Y eso tú como lo sabes?"
		},
		{
			"choice": "[ Esperar pacientemente a que se calme ]",
			"condition": "",
			"definition": "",
			"event_id": "dialogic_011",
			"value": ""
		},
		{
			"character": "",
			"event_id": "dialogic_001",
			"portrait": "",
			"text": "Ves que recupera la compostura."
