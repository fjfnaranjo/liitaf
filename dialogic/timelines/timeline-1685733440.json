			"operation": "+",
			"random_upper_limit": 100,
			"set_random": false,
			"set_value": "1"
		},
		{
			"choice": "Rodeándome de la gente adecuada, todo saldrá bien.",
			"condition": "",
			"definition": "",
			"event_id": "dialogic_011",
			"value": ""
		},
		{
			"definition": "1685873489-648",
